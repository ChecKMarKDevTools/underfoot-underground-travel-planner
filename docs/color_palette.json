{
  "color": {
    "$type": "color",

    "brand": {
      "$type": "color",

      "primary": {
        "$type": "color",
        "50": { "$value": "#D4E4FD" },
        "200": { "$value": "#98BFFC" },
        "300": { "$value": "#79ACFC" },
        "400": { "$value": "#5A99FD" },
        "500": { "$value": "#8657D3" },
        "600": { "$value": "#1A6AE8" },
        "700": { "$value": "#1B55B2" },
        "800": { "$value": "#19407C" },
        "900": { "$value": "#14294B" },
        "950": { "$value": "#0D1B31" },
        "base": { "$value": "{color.brand.primary.500}" }
      },

      "secondary": {
        "$type": "color",
        "50": { "$value": "#E8DBF7" },
        "200": { "$value": "#C9AAEF" },
        "300": { "$value": "#B990EB" },
        "400": { "$value": "#AA77E7" },
        "500": { "$value": "#E6679E" },
        "600": { "$value": "#7F4FC5" },
        "700": { "$value": "#613E9A" },
        "800": { "$value": "#452D70" },
        "900": { "$value": "#2B1D47" },
        "950": { "$value": "#1C132E" },
        "base": { "$value": "{color.brand.secondary.500}" }
      },

      "success": {
        "$type": "color",
        "50": { "$value": "#B6FBEA" },
        "200": { "$value": "#3EF9CA" },
        "300": { "$value": "#05F5B8" },
        "400": { "$value": "#02BA8C" },
        "500": { "$value": "#007F5F" },
        "600": { "$value": "#0A644D" },
        "700": { "$value": "#0C503F" },
        "800": { "$value": "#0D3E32" },
        "900": { "$value": "#0C2E25" },
        "950": { "$value": "#081E18" },
        "base": { "$value": "{color.brand.success.500}" }
      },

      "warning": {
        "$type": "color",
        "50": { "$value": "#FBDBCC" },
        "200": { "$value": "#F9A77F" },
        "300": { "$value": "#F98C58" },
        "400": { "$value": "#FA7130" },
        "500": { "$value": "#FB5607" },
        "600": { "$value": "#C04D16" },
        "700": { "$value": "#934018" },
        "800": { "$value": "#693117" },
        "900": { "$value": "#422212" },
        "950": { "$value": "#2B160C" },
        "base": { "$value": "{color.brand.warning.500}" }
      },

      "error": {
        "$type": "color",
        "50": { "$value": "#FBD1E4" },
        "200": { "$value": "#F78EBE" },
        "300": { "$value": "#F76CAB" },
        "400": { "$value": "#F64998" },
        "500": { "$value": "#F72585" },
        "600": { "$value": "#CF1C6E" },
        "700": { "$value": "#9E1D58" },
        "800": { "$value": "#6F1B41" },
        "900": { "$value": "#45152B" },
        "950": { "$value": "#2D0E1C" },
        "base": { "$value": "{color.brand.error.500}" }
      }
    },

    "neutral": {
      "$type": "color",
      "50": { "$value": "#FFFFFF" },
      "200": { "$value": "#CBCBCB" },
      "300": { "$value": "#B0B0B0" },
      "400": { "$value": "#969696" },
      "500": { "$value": "#7B7B7B" },
      "600": { "$value": "#616161" },
      "700": { "$value": "#464646" },
      "800": { "$value": "#2C2C2C" },
      "900": { "$value": "#111111" },
      "950": { "$value": "#0B0B0B" }
    },

    "specials": {
      "$type": "color",
      "logo-neon-cyan": { "$value": "#3BB8C8" },
      "logo-neon-magenta": { "$value": "#E6679E" },
      "logo-plum": { "$value": "#8657D3" },
      "logo-indigo": { "$value": "#3B2E73" }
    },

    "foreground": {
      "$type": "color",
      "on-primary": { "$value": "#FFFFFF" },
      "on-primary-muted": { "$value": "#FFFFFFB3" },
      "on-secondary": { "$value": "#FFFFFF" },
      "on-secondary-muted": { "$value": "#FFFFFFB3" },
      "on-success": { "$value": "#FFFFFF" },
      "on-success-muted": { "$value": "#FFFFFFB3" },
      "on-warning": { "$value": "#FFFFFF" },
      "on-warning-muted": { "$value": "#FFFFFFB3" },
      "on-error": { "$value": "#FFFFFF" },
      "on-error-muted": { "$value": "#FFFFFFB3" },
      "on-surface": { "$value": "{color.neutral.900}" },
      "on-surface-muted": { "$value": "{color.neutral.600}" },
      "on-inverse": { "$value": "{color.neutral.50}" }
    },

    "action": {
      "$type": "color",
      "primary": {
        "$type": "color",
        "default": { "$value": "{color.brand.primary.500}" },
        "hover": { "$value": "{color.brand.primary.600}" },
        "pressed": { "$value": "{color.brand.primary.700}" }
      },
      "accent": {
        "$type": "color",
        "default": { "$value": "{color.brand.secondary.500}" },
        "hover": { "$value": "{color.brand.secondary.600}" },
        "pressed": { "$value": "{color.brand.secondary.700}" }
      }
    },

    "overlay": {
      "$type": "color",
      "scrim-32": { "$value": "#00000052" },
      "scrim-40": { "$value": "#00000066" },
      "scrim-56": { "$value": "#0000008F" },
      "focus-ring": { "$value": "{color.brand.primary.500}" },
      "selection": { "$value": "{color.brand.primary.200}" },
      "highlight": { "$value": "{color.brand.secondary.200}" }
    },

    "static": {
      "$type": "color",
      "white": { "$value": "#FFFFFF" },
      "black": { "$value": "#000000" },
      "transparent": { "$value": "#00000000" }
    },

    "semantic": {
      "$type": "color",
      "bg": { "$value": "{color.neutral.50}" },
      "surface": { "$value": "{color.neutral.200}" },
      "text": { "$value": "{color.neutral.900}" },
      "muted": { "$value": "{color.neutral.600}" },
      "border": { "$value": "{color.neutral.300}" },
      "link": { "$value": "{color.brand.primary.500}" },
      "success": { "$value": "{color.brand.success.500}" },
      "warning": { "$value": "{color.brand.warning.500}" },
      "error": { "$value": "{color.brand.error.500}" },
      "accent": { "$value": "{color.brand.secondary.500}" }
    },

    "modes": {
      "light": {
        "$type": "color",
        "bg": { "$value": "{color.neutral.50}" },
        "surface": { "$value": "{color.neutral.200}" },
        "text": { "$value": "{color.neutral.900}" },
        "muted": { "$value": "{color.neutral.600}" },
        "border": { "$value": "{color.neutral.300}" },
        "link": { "$value": "{color.brand.primary.600}" },
        "on-surface": { "$value": "{color.foreground.on-surface}" }
      },
      "dark": {
        "$type": "color",
        "bg": { "$value": "{color.neutral.900}" },
        "surface": { "$value": "{color.neutral.800}" },
        "text": { "$value": "{color.neutral.50}" },
        "muted": { "$value": "{color.neutral.400}" },
        "border": { "$value": "{color.neutral.700}" },
        "link": { "$value": "{color.brand.primary.300}" },
        "on-surface": { "$value": "{color.neutral.50}" }
      }
    }
  }
}
